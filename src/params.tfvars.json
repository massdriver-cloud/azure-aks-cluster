{
  "kubernetes_version": "1.23.3",
  "node_groups": [{
    "name": "michael",
    "node_size": "D4s (4 vCores, 16 GiB memory)",
    "max_size": 100,
    "min_size": 1
  },
  {
    "name": "michael2",
    "node_size": "B2ms (2 vCores, 8 GiB memory)",
    "max_size": 100,
    "min_size": 1
    }],
  "md_metadata": {
    "name_prefix": "local-dev-aks-0000",
    "default_tags": {
      "md-project": "local",
      "md-target": "dev",
      "md-manifest": "aks"
    }
  },
  "core_services": {
    "enable_ingress": true,
    "azure_dns_zones": {
      "dns_zones": ["mdazuresbx.com"],
      "resource_group": "mdazuresbx-domain"
    }
  }
}
